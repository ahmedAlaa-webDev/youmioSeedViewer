{
  "manifest_version": 3,
  "name": "Youmio Seed Viewer",
  "version": "1.0.0",
  "description": "View Youmio Seed NFTs (Mythic & Ancient) with staking points from OpenSea",
  "permissions": [
    "sidePanel"
  ],
  "host_permissions": [
    "https://api.opensea.io/*",
    "https://staking.youmio.ai/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "panel.html"
  },
  "commands": {
    "show-nfts": {
      "suggested_key": {
        "default": "Ctrl+Shift+N",
        "mac": "Command+Shift+N"
      },
      "description": "Show NFTs for Sale"
    }
  },
  "action": {
    "default_title": "Youmio Seed Viewer"
  },
  "icons": {

  }
}